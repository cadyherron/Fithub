<div class="page-header">
  <h1>New Meal</h1>
</div>

<div class= "row">
  <div class="col-sm-6 form-group">
    <%= form_for @meal do |meal| %>
      <p>
        <%= meal.label :meal_type %>
        <%= meal.text_field :meal_type, class: "form-control" %>
      </p>
      <p>
        <%= meal.submit "Create Meal", class: "btn btn-primary" %>
      </p>
    <% end %>
  </div>
</div>
